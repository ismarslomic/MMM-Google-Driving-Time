{
  "root": true,
  "extends": [
    "airbnb-typescript-prettier"
  ],
  "ignorePatterns": [
    "*.js"
  ],
  "rules": {
    "newline-before-return": "warn",
    "no-restricted-syntax": "off",
    "import/no-unresolved": "off",
    "no-await-in-loop": "warn",
    "lines-between-class-members": "off",
    "class-methods-use-this": "warn"
  }
}
